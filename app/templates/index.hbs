<div class="index-header">
  <img src="" alt="mountains" />
</div>
<div class="index-body">
  <div class="row">
    {{#each model as |post|}}
    <div class="col-sm-6">
      <h5>{{post.title}}</h5>
      <h6>{{post.date}}</h6>
      <img class="post-img" src="{{post.image}}" alt="stock photo">
      <hr>
    </div>
    {{/each}}
  </div>
</div>
